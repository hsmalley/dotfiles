# PYTHON
alias pip="/opt/homebrew/bin/python3 -m pip"
alias pip3="pip"
alias python="/opt/homebrew/bin/python3"
alias python3="/opt/homebrew/bin/python3"

alias vim="/opt/homebrew/bin/nvim"
alias radssh="python3 -m radssh.shell"
alias rad="python3 -m radssh.shell --username $(whoami)"
alias ls="lsd"
alias less="bat"

alias java="/opt/homebrew/Cellar/openjdk/21.0.1/bin/java"

alias kubectl="minikube kubectl --"